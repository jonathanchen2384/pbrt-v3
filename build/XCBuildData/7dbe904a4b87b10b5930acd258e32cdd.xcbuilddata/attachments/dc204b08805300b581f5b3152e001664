#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half
  /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/Debug/toFloat > /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/toFloat.h
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half
  /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/Release/toFloat > /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/toFloat.h
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half
  /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/MinSizeRel/toFloat > /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/toFloat.h
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half
  /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/RelWithDebInfo/toFloat > /Users/jonathanchen/Desktop/pbrt-v3/src/ext/openexr/IlmBase/Half/toFloat.h
fi

